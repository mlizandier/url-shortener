generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
}

model ShortUrl {
    shortCode   String   @id @map("short_code")
    originalUrl String   @map("original_url")
    createdAt   DateTime @default(now()) @map("created_at")
    updatedAt   DateTime @updatedAt @map("updated_at")

    @@map("short_url")
}
